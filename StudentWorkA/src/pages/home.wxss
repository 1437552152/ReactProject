.test {
  color: green;
}
.marginDis {
  margin-right: 10px;
}
.load {
  width: 1200px;
  margin: 0 auto;
}
.title {
  color: #333;
  font-size: 18px;
}
.title span {
  color: #666;
  font-size: 12px;
  margin-left: 10px;
}
.box {
  float: left;
  margin-right: 15px;
  width: calc(1.66666667%);
  color: #fff;
  padding: 20px 10px;
  text-align: center;
  border-radius: 5px;
}
.box p:first-child {
  font-size: 16px;
  margin-bottom: 5px;
}
.box p:nth-child(2) {
  font-size: 12px;
  margin-bottom: 0px;
}
.boxone {
  background-color: #279b2c;
}
.boxtwo {
  background-color: #2544e6;
}
.boxthree {
  background-color: #050063;
}
.boxfour {
  background-color: #3e0ca7;
}
.boxfive {
  background-color: #e87a16;
}
.boxsix {
  background-color: #e71b22;
}
.boxContent {
  height: 100px;
}
.project {
  width: 1200px;
  margin: 40px auto 0 auto;
  height: 400px;
}
.guide {
  height: 40px;
}
.guide span:first-child {
  float: left;
  font-size: 18px;
  color: #333;
}
.guide span:nth-child(2) {
  float: right;
  font-size: 12px;
  color: #666;
}
.list {
  margin-top: 10px;
}
.list ul {
  width: 100%;
  height: 100%;
  padding-left: 0;
}
.list ul li {
  float: left;
  display: flex;
  justify-content: space-around;
  width: calc(18.33333333%);
  margin-top: 15px;
}
.list ul li {
  margin-left: 15px;
}
.list ul li div:first-child {
  width: 70px;
  height: 70px;
  border-radius: 50%;
  line-height: 70px;
}
.list ul li div:nth-child(2) {
  margin-left: 15px;
}
.list ul li div img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.list ul li div p {
  color: #333;
}
.warning {
  color: green;
}
.clearfix {
  clear: both;
}
